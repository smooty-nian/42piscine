#include <unistd.h>

void	ft_print_alphabet(char c)
{
	int i;
	i = '97';


	while(i <= '122')
	{
		write(1, &i, 1);
		i++;
	}
	
	int 	main(void)	{
		ft_print_alphabet(void);
		return (0);
	}
